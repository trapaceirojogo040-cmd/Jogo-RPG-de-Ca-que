# Environment variables for Jogo-RPG-de-Ca-que
# Copy this file to .env and fill in your actual secrets.
# IMPORTANT: Never commit the .env file to version control.

# Example for a hypothetical game API key
GAME_API_KEY=your_secret_api_key_here

# Example for a database connection string
DATABASE_URL=postgres://user:password@hostname:port/database_name

