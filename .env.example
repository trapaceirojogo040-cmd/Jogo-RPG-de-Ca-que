# Environment variables for Jogo-RPG-de-Ca-que
# Copy this file to .env and fill in your actual secrets.
# IMPORTANT: Never commit the .env file to version control.

# Credenciais do superusuário (OWNER)
OWNER_EMAIL=changeme_to_owner_email@example.com
OWNER_SENHA=changeme_to_a_secure_password

# Chave secreta para a geração de códigos de confirmação internos do jogo.
# Deve ser uma string longa, aleatória e única.
SENHA_BASE=changeme_to_a_long_random_and_unique_string
